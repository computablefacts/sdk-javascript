{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Node",
    "strict": true,
    "declaration": true,
    "outDir": "./dist",
    "lib": [
      "dom",
      "scripthost",
      "es2015.promise"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts",
      "src/cf.ts",
      "src/http-client.ts"
    ],
    "out": "docs",
    "readme": "none",
    "gitRevision": "master",
    "sort": "source-order"
  }
}
