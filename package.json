{
  "name": "@computablefacts/sdk-javascript",
  "version": "0.2.0",
  "description": "JavaScript SDK to access to ComputableFacts API",
  "main": "dist/cf.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run test:watch",
    "clean": "rm -rf dist",
    "init": "mkdir dist",
    "prebuild": "npm run clean && npm run init",
    "build": "rollup --config",
    "build:watch": "npm run build -- --watch",
    "test": "mocha --compiler js:babel-core ./test/**/*.test.js",
    "test:ts": "ts-mocha ./test/**/*.test.ts",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "gitrepo"
  },
  "keywords": [
    "sdk",
    "javascript"
  ],
  "author": "ComputableFacts",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/register": "^7.14.5",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node-fetch": "^2.5.12",
    "chai": "^4.3.4",
    "core-js": "^3.16.0",
    "mocha": "^9.0.3",
    "node-fetch": "^2.6.1",
    "rollup": "^2.55.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.5"
  }
}
