{
  "name": "@computablefacts/sdk-javascript",
  "version": "0.1.0",
  "description": "JavaScript SDK to access to ComputableFacts API",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "prebuild": "npm run clean && npm run init",
    "build": "babel src -d dist --ignore **/*.test.js",
    "clean": "rm -rf dist",
    "init": "mkdir dist",
    "test": "mocha --compiler js:babel-core ./src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "gitrepo"
  },
  "keywords": [
    "sdk",
    "javascript"
  ],
  "author": "ComputableFacts",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "mocha": "^9.0.3"
  }
}
