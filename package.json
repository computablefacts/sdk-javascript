{
  "name": "@computablefacts/sdk-javascript",
  "version": "0.1.1",
  "description": "JavaScript SDK to access to ComputableFacts API",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run test:watch",
    "clean": "rm -rf dist",
    "init": "mkdir dist",
    "prebuild": "npm run clean && npm run init",
    "build": "set BABEL_ENV=build&&babel src -d dist --ignore **/*.test.js",
    "build:watch": "npm run build -- --watch",
    "test": "set BABEL_ENV=test&& mocha --compiler js:babel-core ./src/**/*.test.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "gitrepo"
  },
  "keywords": [
    "sdk",
    "javascript"
  ],
  "author": "ComputableFacts",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "core-js": "^3.16.0",
    "mocha": "^9.0.3",
    "node-fetch": "^2.6.1"
  }
}
